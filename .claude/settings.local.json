{
  "permissions": {
    "allow": [
      "Bash(npx create-next-app:*)",
      "Bash(npx create-next-app@latest:*)",
      "Bash(npm install:*)",
      "Bash(timeout 15 npm run dev:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(npx vitest run:*)",
      "Bash(npx next build:*)",
      "Bash(gh repo view:*)",
      "Bash(gh repo create:*)",
      "Bash(git checkout:*)",
      "Bash(git merge:*)",
      "Bash(git branch:*)",
      "Bash(git push:*)",
      "mcp__Claude_Preview__preview_start",
      "Bash(timeout 10 npm run dev:*)",
      "Bash(npm run dev:*)",
      "Bash(curl:*)",
      "Bash(taskkill:*)",
      "Bash(where:*)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:docs.openclaw.ai)",
      "WebFetch(domain:deepwiki.com)",
      "Bash(node --env-file=.env.local scripts/explore-agent.mjs:*)",
      "Bash(npx tsc:*)"
    ]
  },
  "hooks": {
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "curl -sf -X POST -H \"Content-Type: application/json\" -H \"X-Emdash-Token: $EMDASH_HOOK_TOKEN\" -H \"X-Emdash-Pty-Id: $EMDASH_PTY_ID\" -H \"X-Emdash-Event-Type: notification\" -d @- \"http://127.0.0.1:$EMDASH_HOOK_PORT/hook\" || true"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "curl -sf -X POST -H \"Content-Type: application/json\" -H \"X-Emdash-Token: $EMDASH_HOOK_TOKEN\" -H \"X-Emdash-Pty-Id: $EMDASH_PTY_ID\" -H \"X-Emdash-Event-Type: stop\" -d @- \"http://127.0.0.1:$EMDASH_HOOK_PORT/hook\" || true"
          }
        ]
      }
    ]
  }
}
